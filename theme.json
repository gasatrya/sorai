          "fontSize": "var:preset|font-size|medium",
          "fontWeight": "650"
        }
      },
      "core/site-tagline": {
        "color": {
          "text": "var:preset|color|muted"
        },
        "typography": {
          "fontSize": "var:preset|font-size|tiny",
          "fontWeight": "400"
        }
      },
      "core/navigation": {
        "css": "& a:hover { color: var(--wp--preset--color--tertiary) !important; }",
        "elements": {
          "link": {
            "color": {
              "text": "var:preset|color|secondary"
            },
            ":hover": {
              "color": {
                "text": "var:preset|color|tertiary"
              }
            }
          }
        },
        "typography": {
          "fontSize": "var:preset|font-size|tiny",
          "fontWeight": "550"
        }
      },
      "core/post-featured-image": {
        "css": "&:hover img { filter: none !important; }"
      },
      "core/separator": {
        "color": {
          "background": "var:preset|color|border"
        },
        "spacing": {
          "margin": {
            "top": "var:preset|spacing|30",
            "bottom": "var:preset|spacing|30"
          }
        },
        "border": {
          "width": ".5px"
        },
        "css": " &:not(.is-style-wide):not(.is-style-dots):not(.alignwide):not(.alignfull){width: var(--wp--preset--spacing--60)}"
      },
      "core/read-more": {
        "typography": {
          "fontSize": "var:preset|font-size|tiny",
          "fontWeight": "550"
        },
        "border": {
          "bottom": {
            "width": "0.1rem",
            "style": "solid",
            "color": "transparent"
          }
        },
        "css": "{transition: border 200ms ease-in-out;padding-bottom: 0.1rem;} &:hover { border-color: var(--wp--preset--color--tertiary) !important; } &::after { content: '→'; margin-left: 0.5em; transition: margin-left 200ms ease-in-out; } &:hover::after { margin-left: 0.75em; }"
      },
      "core/query-pagination-next": {
        "typography": {
          "fontSize": "var:preset|font-size|tiny",
          "fontWeight": "550"
        },
        "border": {
          "bottom": {
            "width": "0.1rem",
            "style": "solid",
            "color": "transparent"
          }
        },
        "css": " &:hover { border-color: var(--wp--preset--color--tertiary) !important; } &::after { content: '→'; margin-left: 0.5em; transition: margin-left 200ms ease-in-out; } &:hover::after { margin-left: 0.75em; }"
      },
      "core/query-pagination-previous": {
        "typography": {
          "fontSize": "var:preset|font-size|tiny",
          "fontWeight": "550"
        },
        "border": {
          "bottom": {
            "width": "0.1rem",
            "style": "solid",
            "color": "transparent"
          }
        },
        "css": " &:hover { border-color: var(--wp--preset--color--tertiary) !important; } &::before { content: '←'; margin-right: 0.5em; transition: margin-right 200ms ease-in-out; } &:hover::before { margin-right: 0.75em; }"
      },
      "core/query-pagination-numbers": {
        "typography": {
          "fontSize": "var:preset|font-size|tiny",
          "fontWeight": "550"
        }
      },
      "core/post-excerpt": {
        "typography": {
          "fontSize": "var:preset|font-size|small",
          "lineHeight": "1.6"
        },
        "color": {
          "text": "var:preset|color|muted"
        }
      },
      "core/post-date": {
        "typography": {
          "fontSize": "var:preset|font-size|tiny",
          "fontWeight": "400",
          "fontStyle": "normal"
        },
        "color": {
          "text": "var:preset|color|muted"
        }
      },
      "core/post-author": {
        "typography": {
          "fontSize": "var:preset|font-size|tiny",
          "fontWeight": "400",
          "fontStyle": "normal"
        },
        "color": {
          "text": "var:preset|color|muted"
        },
        "css": "{ align-items: center } & img { display: block; border-radius: 50%; width: 2rem; height: 2rem; object-fit: cover; } & .wp-block-post-author__avatar { margin-right: 0.5rem; }"
      },
      "core/code": {
        "border": {
          "color": "var(--wp--preset--color--border)"
        },
        "color": {
          "background": "var(--wp--preset--color--muted-light)"
        },
        "spacing": {
          "padding": {
            "bottom": "var(--wp--preset--spacing--10)",
            "left": "var(--wp--preset--spacing--10)",
            "right": "var(--wp--preset--spacing--10)",
            "top": "var(--wp--preset--spacing--10)"
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)",
          "fontStyle": "normal",
          "fontWeight": "400",
          "lineHeight": "1.6"
        }
      },
      "core/post-terms": {
        "typography": {
          "fontSize": "var:preset|font-size|tiny",
          "fontWeight": "400",
          "fontStyle": "normal"
        },
        "color": {
          "text": "var:preset|color|muted"
        }
      },
      "core/heading": {
        "spacing": {
          "margin": {
            "top": "var:preset|spacing|30",
            "bottom": "0"
          }
        }
      },
      "core/post-navigation-link": {
        "typography": {
          "fontSize": "var:preset|font-size|tiny"
        },
        "css": "& .post-navigation-link__label { font-weight: 550; color: var(--wp--preset--color--muted); display: block; }"
      },
      "core/comment-author-name": {
        "css": "& a { color: inherit; } & a:hover { color: var(--wp--preset--color--tertiary) !important; }"
      },
      "core/comment-date": {
        "css": "& a { color: var(--wp--preset--color--muted); font-size: var(--wp--preset--font-size--tiny); font-weight: 450; } & a:hover { color: var(--wp--preset--color--tertiary) !important; }"
      },
      "core/comment-edit-link": {
        "css": "& a { color: var(--wp--preset--color--accent); font-size: var(--wp--preset--font-size--tiny) } & a:hover { color: var(--wp--preset--color--tertiary) !important; }"
      },
      "core/comment-reply-link": {
        "css": "& a { color: var(--wp--preset--color--accent); font-size: var(--wp--preset--font-size--tiny) } & a:hover { color: var(--wp--preset--color--tertiary) !important; }"
      },
      "core/post-comments-form": {
        "css": "& textarea, input[type='text'], input[type='email'], input[type='url'] { border: 1px solid var(--wp--preset--color--border); background: var(--wp--preset--color--white); padding: 0.5em; } & label { font-weight: 550; font-size: var(--wp--preset--font-size--tiny); } & .comment-notes, .logged-in-as { font-size: var(--wp--preset--font-size--tiny); color: var(--wp--preset--color--muted); margin-top: 0.25rem; }"
      },
      "core/comments-pagination": {
        "typography": {
          "fontSize": "var:preset|font-size|tiny"
        }
      },
      "core/comments-pagination-numbers": {
        "css": " display: flex; gap: 0.5rem; "
      },
      "core/footnotes": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--tiny)"
        }
      },
      "core/search": {
        "css": " & input[type='search'] { border: 1px solid var(--wp--preset--color--border); background: var(--wp--preset--color--white); padding: 0.5em; width: 100%; font-size: var(--wp--preset--font-size--tiny) } & .wp-block-search__label { font-weight: 550; font-size: var(--wp--preset--font-size--tiny); margin-bottom: 0.25rem; display: block;"
      },
      "core/list": {
        "css": " & ul { padding-left: var(--wp--preset--spacing--10); } "
      },
      "core/pullquote": {
        "elements": {
          "cite": {
            "typography": {
              "fontFamily": "var:preset|font-family|manrope",
              "fontSize": "var:preset|font-size|tiny",
              "fontStyle": "normal"
            }
          }
        },
        "spacing": {
          "padding": {
            "bottom": "var:preset|spacing|20",
            "top": "var:preset|spacing|20"
          }
        },
        "typography": {
          "fontFamily": "Georgia, serif",
          "fontSize": "var:preset|font-size|xx-large",
          "fontStyle": "italic",
          "fontWeight": "400",
          "letterSpacing": "0",
          "lineHeight": "1.5"
        }
      },
      "core/quote": {
        "border": {
          "style": "solid",
          "width": "0 0 0 2px",
          "color": "currentColor"
        },
        "spacing": {
          "blockGap": "var:preset|spacing|10",
          "margin": {
            "left": "0",
            "right": "0"
          },
          "padding": {
            "top": "0.5rem",
            "right": "var:preset|spacing|20",
            "bottom": "0.5rem",
            "left": "var:preset|spacing|20"
          }
        },
        "css": " & cite { font-size: var(--wp--preset--font-size--tiny); font-style: normal; } "
      }
    },
    "spacing": {
      "blockGap": "1.2rem",
      "padding": {
        "left": "var:preset|spacing|30",
        "right": "var:preset|spacing|30"
      }
    },
    "css": ":where(.wp-site-blocks *:focus){outline-width:2px;outline-style:solid}"
  },
  "templateParts": [
    {
      "area": "header",
      "name": "header",
      "title": "Header"
    },
    {
      "area": "footer",
      "name": "footer",
      "title": "Footer"
    }
  ],
  "version": 3,
  "$schema": "https://schemas.wp.org/wp/6.8/theme.json"
}
