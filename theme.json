  "styles": {
    "color": {
      "background": "var:preset|color|background",
      "text": "var:preset|color|foreground"
    },
    "typography": {
      "fontFamily": "var:preset|font-family|manrope",
      "fontSize": "var:preset|font-size|small",
      "fontWeight": "450",
      "lineHeight": "1.65",
      "fontStyle": "normal"
    },
    "elements": {
      "caption": {
        "color": {
          "text": "var:preset|color|muted"
        },
        "typography": {
          "fontSize": "var:preset|font-size|tiny",
          "fontStyle": "italic",
          "lineHeight": "1.4"
        }
      },
      "h1": {
        "typography": {
          "fontSize": "var:preset|font-size|xxxxx-large"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var:preset|font-size|xxxx-large"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var:preset|font-size|xxx-large"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var:preset|font-size|xx-large"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var:preset|font-size|x-large"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "var:preset|font-size|large"
        }
      },
      "heading": {
        "color": {
          "text": "var:preset|color|primary"
        },
        "typography": {
          "fontWeight": "550",
          "lineHeight": "1.35",
          "letterSpacing": "-0.025em"
        }
      },
      "link": {
        "css": "transition: color 200ms ease-in-out;",
        "color": {
          "text": "var:preset|color|accent"
        },
        ":hover": {
          "color": {
            "text": "var:preset|color|primary"
          }
        },
        "typography": {
          "fontWeight": "550",
          "textDecoration": "none"
        }
      },
      "button": {
        "css": "transition: background-color 200ms ease-in-out;",
        "typography": {
          "fontSize": "var:preset|font-size|tiny",
          "fontStyle": "normal",
          "fontWeight": "500"
        },
        "border": {
          "radius": "0"
        },
        "spacing": {
          "padding": {
            "bottom": "var:preset|spacing|5",
            "left": "var:preset|spacing|10",
            "right": "var:preset|spacing|10",
            "top": "var:preset|spacing|5"
          }
        },
        "shadow": "var:preset|shadow|lg",
        "color": {
          "background": "var:preset|color|primary",
          "text": "var:preset|color|background"
        },
        ":hover": {
          "color": {
            "background": "var:preset|color|secondary",
            "text": "var:preset|color|background"
          },
          "border": {
            "color": "transparent"
          }
        }
      }
    },
    "blocks": {
      "core/post-title": {
        "typography": {
          "fontSize": "var:preset|font-size|display-small",
          "lineHeight": "1.2"
        },
        "elements": {
          "link": {
            "color": {
              "text": "var:preset|color|foreground"
            },
